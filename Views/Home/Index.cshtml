        <section class="section bg-color-grey-scale-1 border-0 m-0">
            <div class="container">
                <div class="row">
                    <div class="col text-center">
                        <h2 class="font-weight-normal mb-5">Life-skills workshops </h2>
                    </div>
                </div>
                <div class="container py-4">

                    <div class="row">
                        <div class="col">
                            <div class="blog-posts">

                                <div class="row">
                                    @foreach (var item in Dtype.Take(3))
                                    {
                                        if (item.WorkshopFileTypeID == 3)
                                        {
                                            <div class="col-md-4">
                                                <article class="post post-medium border-0 pb-0 mb-5">
                                                    <div class="post-image">
                                                        <a href="#">
                                                            <img style="height:260px; width:550px" src="~/Uploads/Workshops/@item.fileurl" class="img-fluid img-thumbnail img-thumbnail-no-bordersd rounded-0" alt="" />
                                                        </a>
                                                    </div>

                                                    <div class="post-content">

                                                        <h2 class="font-weight-semibold text-5 line-height-6 mt-3 mb-2"><a href="#">@item.WorkshopTitle</a></h2>
                                                        @Html.Raw(item.WorkshopContent)

                                                        <div class="post-meta">


                                                        </div>

                                                    </div>
                                                </article>
                                            </div>
                                        }
                                        else if (item.WorkshopFileTypeID == 1)
                                        {
                                            <div class="col-md-4">
                                                <article class="post post-medium border-0 pb-0 mb-5">
                                                    <div class="post-image">
                                                        <a href="#">
                                                            <div class="embed-responsive embed-responsive-16by9">
                                                                <iframe class="embed-responsive-item" src="@item.fileurl" width="640" height="360" allowfullscreen></iframe>
                                                            </div>
                                                        </a>
                                                    </div>

                                                    <div class="post-content">

                                                        <h2 class="font-weight-semibold text-5 line-height-6 mt-3 mb-2"><a href="#">@item.WorkshopTitle</a></h2>
                                                        @Html.Raw(item.WorkshopContent)

                                                        <div class="post-meta">

                                                        </div>

                                                    </div>
                                                </article>
                                            </div>

                                        }
                                        else
                                        {
                                            <div class="col-md-4">
                                                <article class="post post-medium border-0 pb-0 mb-5">
                                                    <div class="post-image">
                                                        <div class="post-image">
                                                            <div class="embed-responsive embed-responsive-16by9">
                                                                <iframe class="embed-responsive-item" src="@item.fileurl" width="640" height="360" allowfullscreen></iframe>
                                                            </div>
                                                        </div>
                                                    </div>

                                                    <div class="post-content">

                                                        <h2 class="font-weight-semibold text-5 line-height-6 mt-3 mb-2"><a href="#">@item.WorkshopTitle</a></h2>
                                                        @Html.Raw(item.WorkshopContent)

                                                        <div class="post-meta">

                                                        </div>

                                                    </div>
                                                </article>
                                            </div>

                                        }

                                    }

                                </div>



                            </div>
                        </div>

                    </div>

                </div>
            </div>
        </section>

